digraph RDG {
    rankdir=LR;
    node [fontname="Arial"];
    edge [fontname="Arial", fontsize=10];
    "ForwardPass" [shape=rectangle, style=filled, fillcolor=orange, label="ForwardPass"];
    "ForwardPass" -> "ColorTex" [label="Color", color=red];
    "ForwardPass" -> "DepthTex" [label="Depth", color=red];
    "UniformBuf" -> "ForwardPass" [label="Read", color=blue];
    "PostProcess" [shape=rectangle, style=filled, fillcolor=yellow, label="PostProcess"];
    "PostProcess" -> "OutputTex" [label="UAV", color=red];
    "ColorTex" -> "PostProcess" [label="SRV", color=blue];
    "ColorTex" [shape=box, style=filled, fillcolor=lightgreen, label="ColorTex\nTexture"];
    "DepthTex" [shape=box, style=filled, fillcolor=lightgreen, label="DepthTex\nTexture"];
    "OutputTex" [shape=box, style=filled, fillcolor=lightgreen, label="OutputTex\nTexture"];
    "UniformBuf" [shape=cylinder, style=filled, fillcolor=lightcyan, label="UniformBuf\nBuffer"];
}
